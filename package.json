{
  "name": "vite-react-motoko",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup": "npm i && dfx canister create internet_identity --specified-id rdmx6-jaaaa-aaaaa-aaadq-cai && dfx deploy",
    "start": "run-p auth frontend backend backend_ai",
    "frontend": "vite --port 3000 --host",
    "backend": "mo-dev --generate --deploy -y",
    "auth": "mo-dev --generate --deploy -y",
    "backend_ai": "cargo-watch -w 'src' -q -C ./backend_ai -s 'cd .. && dfx generate && dfx deploy backend_ai -q -y'",
    "build": "tsc && vite build",
    "test": "run-s test:backend test:frontend",
    "test:frontend": "vitest run",
    "test:backend": "mo-test",
    "format": "prettier --write \"src/**/*.{json,js,jsx,ts,tsx,css,scss}\" \"backend/**/*.mo\"",
    "sources": "mops sources",
    "postinstall": "mops install"
  },
  "dependencies": {
    "@dfinity/auth-client": "^2.3.0",
    "@hookform/resolvers": "^4.1.2",
    "@ic-reactor/react": "^1.14.6",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.6",
    "@tailwindcss/vite": "^4.0.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "country-data-list": "^1.4.0",
    "lucide-react": "^0.475.0",
    "motion": "^12.4.7",
    "next-themes": "^0.4.4",
    "react": "^18.3.1",
    "react-circle-flags": "^0.0.23",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.15.1",
    "react-router": "^7.2.0",
    "sonner": "^2.0.1",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.13.4",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "cross-fetch": "^4.0.0",
    "dotenv": "^16.4.5",
    "ic-mops": "^1.1.1",
    "jsdom": "^22.1.0",
    "mo-dev": "^0.13.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.2",
    "prettier": "^3.4.1",
    "prettier-plugin-motoko": "^0.10.1",
    "sass": "^1.81.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite": "^6.0.1",
    "vite-plugin-environment": "^1.1.3",
    "vitest": "^2.1.6",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  }
