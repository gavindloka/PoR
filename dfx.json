{
  "canisters": {
    "backend": {
      "type": "motoko",
      "main": "backend/Backend.mo",
      "declarations": {
        "node_compatibility": true
      }
    },
    "frontend": {
      "dependencies": [
        "backend"
      ],
      "type": "assets",
      "source": [
        "dist/"
      ]
    },
    "backend_ai": {
      "metadata": [
        {
          "name": "candid:service"
        }
      ],
      "candid": "backend_ai/backend.did",
      "package": "backend_ai",
      "type": "custom",
      "wasm": "target/wasm32-wasi/release/backend_ai-ic.wasm",
      "build": [
        "bash build.sh"
      ]
    }
  },
  "defaults": {
    "build": {
      "packtool": "npm run --silent sources"
    }
  },
  "output_env_file": ".env",
  "version": 2
}